    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-polyfill": "^6.16.0",
    "babel-preset-es2015": "^6.18.0",
    "babel-runtime": "^6.23.0",
    "chai": "^3.5.0",
    "chance": "^1.0.6",
    "console.mute": "^0.3.0",
    "eslint": "^3.11.1",
    "eslint-plugin-import": "^2.2.0",
    "istanbul": "^1.1.0-alpha.1",
    "jsdoc": "^3.4.3",
    "npm-run-all": "^4.0.2",
    "nyc": "^10.1.2",
    "rewire": "^2.5.2",
    "sinon": "^1.17.6"
  },
  "dependencies": {
    "joi": "^10.2.2",
    "lodash": "^4.17.4",
