{
  "name": "architect",
  "version": "1.0.0",
  "description": "",
  "main": "lib/",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "test": "npm-run-all test:report coverage",
    "test:unit": "ava test/unit/**/ test/unit/ test/functional/**/ test/functional/",
    "test:report": "nyc npm-run-all test:unit",
    "coverage": "nyc report --reporter=text",
    "jsdoc": "npm-run-all --parallel jsdoc:*",
    "jsdoc:html": "jsdoc -r -d doc/html lib",
    "lint": "npm-run-all lint:fix lint:verify",
    "lint:verify": "eslint lib test",
    "lint:fix": "eslint --fix lib test"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "ava": "^0.18.2",
    "babel-cli": "^6.18.0",
